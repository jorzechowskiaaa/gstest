<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_Select_Territory_and_Resource</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK(Service_Territory__c)), NOT(ISBLANK(Service_Resource__c)))</errorConditionFormula>
    <errorMessage>You cannot select both a Service Territory and Service Resource on the same Override Preference</errorMessage>
</ValidationRule>
