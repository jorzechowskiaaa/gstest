<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Must_Select_Either_Territory_or_Resource</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISBLANK(Service_Territory__c), ISBLANK(Service_Resource__c))</errorConditionFormula>
    <errorMessage>You must select either a Service Territory or Service Resource</errorMessage>
</ValidationRule>
