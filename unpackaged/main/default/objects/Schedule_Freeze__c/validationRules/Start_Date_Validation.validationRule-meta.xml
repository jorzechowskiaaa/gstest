<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_Date_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Start_Date__c &gt; End_Date__c,Start_Date__c == End_Date__c)</errorConditionFormula>
    <errorMessage>Start Date should be never Greater than OR Equal End Date</errorMessage>
</ValidationRule>
