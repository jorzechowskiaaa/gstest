<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>StartDateEndDateNotEmpty</fullName>
    <active>true</active>
    <errorConditionFormula>OR(ISBLANK( Start_Date__c) , ISBLANK(End_Date__c))</errorConditionFormula>
    <errorMessage>Start and End dates should not be blank</errorMessage>
</ValidationRule>
