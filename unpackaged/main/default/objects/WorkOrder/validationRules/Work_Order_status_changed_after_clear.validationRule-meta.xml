<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Work_Order_status_changed_after_clear</fullName>
    <active>false</active>
    <description>Work order status CANT  be changed after call clear.</description>
    <errorConditionFormula>PRIORVALUE( Status ) == &#39;Cleared&#39; &amp;&amp;  ISCHANGED(Status)</errorConditionFormula>
    <errorDisplayField>Status</errorDisplayField>
    <errorMessage>Work order status CANT  be changed after call clear.</errorMessage>
</ValidationRule>
