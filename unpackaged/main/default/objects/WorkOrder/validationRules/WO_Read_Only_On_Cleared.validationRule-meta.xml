<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>WO_Read_Only_On_Cleared</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISPICKVAL(PRIORVALUE(Status),&quot;Cleared&quot;), NOT($Profile.Name=&quot;System Administrator&quot;))</errorConditionFormula>
    <errorMessage>Work Order cannot be updated after status is set to Cleared</errorMessage>
</ValidationRule>
