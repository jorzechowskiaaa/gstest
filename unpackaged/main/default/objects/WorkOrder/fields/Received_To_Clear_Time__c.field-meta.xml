<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Received_To_Clear_Time__c</fullName>
    <description>This field calculates the time in minutes it took to for the status of WO to move from Spotted (created timestamp) to Cleared</description>
    <externalId>false</externalId>
    <formula>if ( Cleared_Timestamp__c  &gt; Spotted_Timestamp__c , ( Cleared_Timestamp__c  - Spotted_Timestamp__c)*1440,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Received To Clear Time</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
