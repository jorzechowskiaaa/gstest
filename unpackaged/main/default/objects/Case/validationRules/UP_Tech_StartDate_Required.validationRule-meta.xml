<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>UP_Tech_StartDate_Required</fullName>
    <active>true</active>
    <description>Requires a Start Date for Technician resources</description>
    <errorConditionFormula>AND( Text(FSL_Profile__c) == &#39;Technician&#39;,  ISBLANK( FSL_Start_Date__c ) )</errorConditionFormula>
    <errorDisplayField>FSL_Start_Date__c</errorDisplayField>
    <errorMessage>Start Date is required for technician profiles</errorMessage>
</ValidationRule>
