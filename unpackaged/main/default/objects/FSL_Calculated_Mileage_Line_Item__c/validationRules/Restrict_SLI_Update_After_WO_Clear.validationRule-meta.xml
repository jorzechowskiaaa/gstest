<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Restrict_SLI_Update_After_WO_Clear</fullName>
    <active>false</active>
    <errorConditionFormula>AND (ISPICKVAL( Work_Order__r.Status , &#39;Cleared&#39;),NOT($Profile.Name=&quot;System Administrator&quot;))</errorConditionFormula>
    <errorMessage>Calculated Mileage line item can not be updated if Work order is Cleared</errorMessage>
</ValidationRule>
