<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FSl_Start_Hour_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>IF( Start_Hour__c  &gt;=  End_Hour__c , TRUE, FALSE)</errorConditionFormula>
    <errorMessage>Start time should not be greater than or equal to End time.</errorMessage>
</ValidationRule>
