<?xml version="1.0" encoding="UTF-8"?>
<WebLink xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Update_Public_Group_Membership</fullName>
    <availability>online</availability>
    <description>Fixes the public group membership to this territory for dispatchers if they have access to this territory through user territories but the public group membership has dropped off</description>
    <displayType>button</displayType>
    <linkType>javascript</linkType>
    <masterLabel>Update Public Group Membership</masterLabel>
    <openType>onClickJavaScript</openType>
    <protected>false</protected>
    <url>{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot;)}

sforce.apex.execute(&quot;FSL_VerifyAddPublicGroupMember&quot;,&quot;giveMembership&quot;,{stId:&quot;{!ServiceTerritory.Id}&quot;});
alert(&quot;Public Group Membership fixed for {!ServiceTerritory.Name}&quot;);</url>
</WebLink>
