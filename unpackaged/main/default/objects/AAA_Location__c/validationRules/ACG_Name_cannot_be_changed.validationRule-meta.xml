<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ACG_Name_cannot_be_changed</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(CONTAINS( $Profile.Name  , &quot;Admin&quot;))
&amp;&amp;
NOT(CONTAINS( $Profile.Name , &quot;Integration&quot;))
&amp;&amp;
ISCHANGED( Name )</errorConditionFormula>
    <errorMessage>The AAA Location Name cannot be changed.</errorMessage>
</ValidationRule>
