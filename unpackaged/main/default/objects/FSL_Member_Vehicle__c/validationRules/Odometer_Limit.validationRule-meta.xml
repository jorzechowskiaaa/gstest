<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Odometer_Limit</fullName>
    <active>false</active>
    <description>Limits length to 6 digits</description>
    <errorConditionFormula>IF( LEN(TEXT(Vehicle_Odometer__c)) &gt; 6 , true, false)</errorConditionFormula>
    <errorMessage>Vehicle odometer can not be greater than 6 digits</errorMessage>
</ValidationRule>
