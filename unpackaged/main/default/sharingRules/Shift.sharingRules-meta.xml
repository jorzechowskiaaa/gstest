<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>Share_Shifts_with_BO</fullName>
        <accessLevel>Edit</accessLevel>
        <description>Share all Shifts with BO with Public Read/Write which in conjunction with Permission Sets will determine the final permission each BO persona will have.</description>
        <label>Share Shifts with BO</label>
        <sharedTo>
            <group>Back_Office_Group</group>
        </sharedTo>
        <criteriaItems>
            <field>ShiftNumber</field>
            <operation>notEqual</operation>
            <value></value>
        </criteriaItems>
        <includeRecordsOwnedByAll>true</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
</SharingRules>
