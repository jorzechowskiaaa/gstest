    <decisions>
        <name>Check_If_Facility_is_Auto_Cal</name>
        <label>Check If Facility is Auto Cal</label>
        <locationX>297</locationX>
        <locationY>503</locationY>
        <defaultConnector>
            <targetReference>Check_If_BA</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>(1 OR 2) AND (3 OR 4)</conditionLogic>
            <conditions>
                <leftValueReference>FacilityCalculatedMileage</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>From Driver Location</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>FacilityCalculatedMileage</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>From Service Territory</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Product__r.ProductCode</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>TW</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Product__r.ProductCode</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>OM</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>TW_and_OM_Is_Auto_Calculated</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <name>Check_XX_Pricing_Conditions</name>
        <label>Check XX Pricing Conditions</label>
        <locationX>451</locationX>
        <locationY>2003</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Satisfies</name>
            <conditionLogic>1 AND (3 OR (4 AND 2)) AND 6 AND (5 OR (7 AND 8))</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Work_Order__r.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Cleared</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Work_Order__r.RAP_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>RAP Cleared</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Work_Order__r.Call_Type__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>RAP</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Work_Order__r.Call_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>RAP</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Work_Order__r.Clearing_Code__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>XX</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>sliTotalPrice</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Work_Order__r.Enroute_Timestamp__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Work_Order__r.Resolution_Code__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>X002</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_XX_Values</targetReference>
            </connector>
            <label>Satisfies</label>
        </rules>
    </decisions>
