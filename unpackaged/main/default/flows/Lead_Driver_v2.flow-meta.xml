    <formulas>
        <name>CalcEndTime</name>
        <dataType>DateTime</dataType>
        <expression>({!$Flow.CurrentDateTime}+({!Enter_ETA_in_Minutes}/1440))+(30/1440)</expression>
    </formulas>
    <formulas>
        <name>CalcStartTime</name>
        <dataType>DateTime</dataType>
        <expression>{!$Flow.CurrentDateTime} + ({!Enter_ETA_in_Minutes} / 1440)</expression>
    </formulas>
    <formulas>
        <name>CalcTowEnd</name>
        <dataType>DateTime</dataType>
        <expression>({!$Flow.CurrentDateTime}+({!Enter_ETA_in_Minutes}/1440))+(60/1440)</expression>
    </formulas>
    <formulas>
        <name>CalcTowStart</name>
        <dataType>DateTime</dataType>
        <expression>({!$Flow.CurrentDateTime}+({!Enter_ETA_in_Minutes}/1440))+(30/1440)</expression>
    </formulas>
