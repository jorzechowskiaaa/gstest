        <rules>
            <name>RAP_Call</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>Get_Case.Call_Origin__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>RAP</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Case.FSL_Work_Order__r.Call_Type__c</leftValueReference>
