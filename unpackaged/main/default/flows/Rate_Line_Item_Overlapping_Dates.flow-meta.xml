    <start>
        <locationX>50</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Get_Sibling_Records</targetReference>
        </connector>
        <filterFormula>OR(ISNEW(), (
OR (ISCHANGED({!$Record.End_Date__c}), 
OR(ISCHANGED({!$Record.Start_Date__c}),
OR(ISCHANGED({!$Record.Time_of_Day_Code__c}),
OR(ISCHANGED({!$Record.Max_Data_Range__c}),
OR(ISCHANGED({!$Record.Min_Data_Range__c}),
