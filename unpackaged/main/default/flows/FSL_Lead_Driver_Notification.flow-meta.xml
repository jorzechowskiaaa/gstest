    <formulas>
        <name>ApptCallNotificationBody</name>
        <dataType>String</dataType>
        <expression>{!$Record.AppointmentNumber}+&quot; Appt Call has been Scheduled to &quot;+ {!$Record.Service_Resource__r.Name} +&quot; at &quot; +  IF( 
OR( 
 VALUE( MID( TEXT( {!$Record.SchedStartTime} -(8/24)), 12, 2 ) ) = 0, 
 VALUE( MID( TEXT( {!$Record.SchedStartTime} -(8/24)), 12, 2 ) ) = 12 
), 
  &quot;12&quot;, 
 TEXT( VALUE( MID( TEXT( {!$Record.SchedStartTime}-(8/24)), 12, 2 ) ) 
 - 
IF( 
VALUE( MID( TEXT( {!$Record.SchedStartTime} -(8/24)), 12, 2 ) ) &lt; 12, 
  0, 
  12 
  ) 
 ) 
) 
&amp; &quot;:&quot; &amp; 
MID( TEXT( {!$Record.SchedStartTime} -(8/24)), 15, 2 ) 
&amp; &quot;:&quot; &amp; 
MID( TEXT( {!$Record.SchedStartTime} -(8/24)), 18, 2 ) 
&amp; &quot; &quot; &amp; 
IF( 
 VALUE( MID( TEXT( {!$Record.SchedStartTime} -(8/24)), 12, 2 ) ) &lt; 12, 
 &quot;AM&quot;, 
 &quot;PM&quot; 
)+&quot; on &quot;+Text(DateValue( {!$Record.SchedStartTime} ))</expression>
    </formulas>
    <formulas>
