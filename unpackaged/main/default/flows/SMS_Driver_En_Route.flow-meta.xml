    <decisions>
        <name>Battery_call</name>
        <label>Battery call?</label>
        <locationX>446</locationX>
        <locationY>816</locationY>
        <defaultConnector>
            <targetReference>Copy_1_of_Is_AA_Link_Populated</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default / Non Battery Call</defaultConnectorLabel>
        <rules>
            <name>yes_battery_call</name>
            <conditionLogic>1 AND 5 AND 10 AND (2 OR 3 OR 4) AND (6 OR 7 OR (8 AND NOT 9))</conditionLogic>
            <conditions>
                <leftValueReference>$Record.WO_Call_Type__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>RAP</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.T_Code__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>L304</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.T_Code__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>L302</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.WorkType.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Battery Test</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>f_timeCheckBatteryLink</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.ServiceTerritory.Facility_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Battery with Full Svc</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.ServiceTerritory.Facility_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Battery with Light Svc</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Service_Resource__r.Vehicle__r.VehicleType__r.Name</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>BAT</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Service_Resource__r.Vehicle__r.VehicleType__r.Name</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue> NO Jump or Battery</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>v_batteryLink</leftValueReference>
                <operator>IsBlank</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Is_AA_Link_Populated</targetReference>
            </connector>
            <label>Battery call</label>
        </rules>
    </decisions>
