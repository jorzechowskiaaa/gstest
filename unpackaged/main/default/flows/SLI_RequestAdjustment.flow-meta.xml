    <decisions>
        <name>Is_Work_Order_cleared</name>
        <label>Is Work Order cleared (OR is club ACA)?</label>
        <locationX>182</locationX>
        <locationY>350</locationY>
        <defaultConnector>
            <targetReference>getSLI_inprogress</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>(1 AND 2) OR 3</conditionLogic>
            <conditions>
                <leftValueReference>Get_Work_Order_status.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Cleared</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_BO_Settings.Club_Code__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>240</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_BO_Settings.Club_Code__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>212</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>getSLI</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
