    <formulas>
        <name>CLTimeDifferenceLST_OL_TMWith15</name>
        <dataType>Boolean</dataType>
        <expression>IF(
	(
		(
			{!$Record.Cleared_Timestamp__c}  - {!$Record.On_Location_Timestamp__c}
		) * 24 * 60
	) &lt; 15, TRUE, FALSE
)</expression>
    </formulas>
    <formulas>
        <name>ClTmSmallThanEmTm</name>
        <dataType>Boolean</dataType>
        <expression>IF(
	(
		(
			{!$Record.Cleared_Timestamp__c}  -  {!$Record.Enroute_Timestamp__c}
		) * 24 * 60
	) &lt; 5, True, False
)</expression>
    </formulas>
    <formulas>
        <name>DiffOfClearTmAndEmTm</name>
        <dataType>Boolean</dataType>
        <expression>IF(
	(
		(
			{!$Record.Cleared_Timestamp__c}  -  {!$Record.Enroute_Timestamp__c}
		) * 24 * 60
	) &gt; 5, True, False
)</expression>
    </formulas>
    <formulas>
        <name>ResolutionCodeFormula</name>
        <dataType>String</dataType>
        <expression>LEFT(TEXT({!$Record.Resolution_Code__c}), 4)</expression>
    </formulas>
