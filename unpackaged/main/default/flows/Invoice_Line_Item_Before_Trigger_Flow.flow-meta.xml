                <rightValue>
                    <stringValue>XX</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>sliTotalPrice</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Invoice__r.Work_Order__r.Resolution_Code__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>X002</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Invoice__r.Work_Order__r.Enroute_Timestamp__c</leftValueReference>
                <operator>IsBlank</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_XX_Values</targetReference>
            </connector>
            <label>Satisfies</label>
        </rules>
    </decisions>
